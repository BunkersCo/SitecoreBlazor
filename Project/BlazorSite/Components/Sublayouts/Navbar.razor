
@using Foundation.BlazorExtensions.Components

@implements Microsoft.AspNetCore.Components.IComponent

<nav class="navbar navbar-expand-md navbar-dark fixed-top bg-dark top-row">
    <div class="container">
        <BlazorPlaceholder Name="navbar-header">
            @ChildContent
        </BlazorPlaceholder>
        <BlazorPlaceholder Name="navbar-menu">
            @ChildContent
        </BlazorPlaceholder>

        <BlazorPlaceholder Name="navbar-activity">
            @ChildContent
        </BlazorPlaceholder>
    </div>

</nav>

@functions  {

    [Parameter]
    protected RenderFragment ChildContent { get; set; }

    [Parameter]
    List<SitecoreBlazorHosted.Shared.Models.IBlazorSitecoreField> FieldsModel { get; set; }

}
