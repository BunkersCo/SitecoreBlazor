@using Foundation.BlazorExtensions.Components

@inherits BaseComponent

<nav class="navbar navbar-expand-md navbar-dark fixed-top bg-dark top-row">
    <div class="container">
        <BlazorPlaceholder Name="navbar-header">
            @ChildContent
        </BlazorPlaceholder>
        <BlazorPlaceholder Name="navbar-menu">
            @ChildContent
        </BlazorPlaceholder>

        <BlazorPlaceholder Name="navbar-activity">
            @ChildContent
        </BlazorPlaceholder>
    </div>

</nav>

@code  {

    [Parameter]
    public  RenderFragment ChildContent { get; set; }
   

}
